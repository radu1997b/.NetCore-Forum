<environment include="Development">
    <link rel="stylesheet" href="~/css/Room/RoomTable.css"/>
</environment>
<environment exclude="Development">
    @section Stylesheets{ 
        <link rel="stylesheet" href="~/css/Room/RoomTable.min.css"/>
    }
</environment>

<div class="container">
<br />
<div style="width:90%; margin:0 auto;">
    <div class="row" asp-validation-summary="All"></div>
    <div class="row">
    </div>
    <br />
    <table id="topic-admin-table" class="table table-striped table-bordered dt-responsive nowrap" cellspacing="0">
        <thead>
            <tr>
                <th>Topic Name</th>
                <th>Number of Rooms</th>
                <th>Created Date</th>
            </tr>
        </thead>
    </table>
</div>
</div>

<environment include="Development">
    @section Scripts{ 
        <script src="~/js/Topic/TopicTableConfig.js"></script>
    }
</environment>
<environment exclude="Development">
    @section Scripts{ 
        <script src="~/js/Topic/TopicTableConfig.min.js"></script>
    }
</environment>

